extends ../layout
block content
    .container(style="margin-bottom: 60px;")
        .row
            .col-lg-7.col-md-12
                embed(src="#{movie.flash}", allowFullScreen="true", quality="high", width="652", height="543", align="middle", type="application/x-shockwave-flash")
                .panel.panel-default(style="margin-top: 10px;")
                    .panel-heading
                        h3 评论区
                    .panel-body
                        ul.media-list
                             each item in comments
                                li.media
                                    .pull-left
                                        a.comment(href="#comments",data-cid="#{item._id}",data-tid="#{item.from._id}")
                                            img.media-object(src="data:image/jpeg;base64,", style="width: 64px; height: 64px;")
                                    .media-body
                                        h4.media-heading #{item.from.name}
                                        p #{item.content}
                                        if item.reply && item.reply.length>0
                                            each reply in item.reply
                                                .media
                                                    .pull-left
                                                        a.comment(href="#comments",data-cid="#{item._id}",data-tid="#{reply.from._id}")
                                                            img.media-object(src="data:image/jpeg;base64,", style="width: 64px; height: 64px;")
                                                    .media-body
                                                        h4.media-heading #{reply.from.name}
                                                            span.text-info &nbsp;回复 #{reply.to.name} :
                                                        p #{reply.content}
                        hr
                        #comments
                            form#commentForm(method="POST", action="/user/comment")
                                input(type="hidden",name="comment[movie]",value="#{movie._id}")
                                if user
                                    input(type="hidden",name="comment[from]",value="#{user._id}")
                                .form-group
                                    textarea#content.form-control(name="comment[content]",row="3")
                                if user
                                    button#submit.btn.btn-primary(type='submit') 提交
                                else
                                    a.navbar-link(href="#",data-toggle="modal",data-target="#signinModal") 登录后评论
                                <br/>
                                <br/>
            .col-lg-5.col-md-12
                dl.dl-horizontal
                    dt 电影名字
                    dd= movie.title
                    dt 导演
                    dd= movie.doctor
                    dt 国家
                    dd= movie.country
                    dt 语言
                    dd= movie.language
                    dt 上映年份
                    dd= movie.year
                    dt 简介
                    dd= movie.summary
    script(src="/js/detail.js")